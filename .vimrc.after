colorscheme zenburn
set guioptions-=L
set wildignore+=*/tmp/*,*/dist/*,*/log/*,*/node_modules/*,*/bower_components/*,*/electron-builds/*,*.so,*.swp,*.zip
set synmaxcol=250
augroup AuNERDTreeCmd
    autocmd!
augroup end
command Sp source ~/.vim/sessions/space-pizzas

let g:syntastic_javascript_checkers = ['eslint']
set expandtab
set tabstop=2
